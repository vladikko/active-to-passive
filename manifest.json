{
  "name": "Passive to Active",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "Passive to Active",
  "browser_action": {
    "default_popup": "popup.html"
 },
  "background": {
    "scripts": ["jquery-3.3.1.min.js","inject.js"],
	"persistent": true
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage",
    "unlimitedStorage",
    "contextMenus"
  ]
}
